@model IList<GroupWebApp.Storage.Entities.Recipe>
    
    

@foreach (var recipe in Model)
{ 
    <h1>@recipe.Name</h1>
     @Html.Raw("<img style='width:500px;align=middle; border-radius: 10px;height:410px; 'src=\"data:image/jpeg;base64,"+Convert.ToBase64String((recipe.Pic))+"\"/>")
     <div class="Recipe__text">@recipe.desc</div>
}
 